
term set opair.abs(set s0, set s1, prop p0) { { (s0, s1) | p0} }
axiom opair.abs.def(prop p0, set s0, set s1, set s2) {
  |- eq(opair.abs(s0, s1, p0), abs(s2, exist(s0, exist(s1, and(eq(s2, opair(s0,s1)), p0)))))
  diff (s2, p0) (s2, s0) (s2, s1)
}

